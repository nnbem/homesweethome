<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="MenuMember-Mapper">
	<select id="selectMainMenu" resultType="menuMember">
		select *
		from m_menu
		where mCode like '%'||'0000'
		order by mCode asc
	</select>

</mapper>