<?xml version="1.0" encoding="UTF-8"?>

<decorators defaultdir="/WEB-INF/decorator">
	<decorator name="staff" page="decorator_staff.jsp">
		<pattern>/staff/*</pattern>
	</decorator>
	
	<decorator name="member" page="decorator_member.jsp">
		<pattern>/member/*</pattern>
	</decorator>
   
	<excludes>
		<pattern>/module/*</pattern>
		<pattern>/member/main</pattern>
		<pattern>/staff/main</pattern>
		<pattern>*/open_regist</pattern>
		<pattern>*/open_modify</pattern>
	</excludes>
</decorators>
