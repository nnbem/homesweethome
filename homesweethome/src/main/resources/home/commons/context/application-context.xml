<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:c="http://www.springframework.org/schema/c"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- member.picture -->
   <bean id="picturePath" 
        class="java.lang.String"
        c:value="${savePath.member.picture}"       
   />
	<!-- log saved path -->
	<bean id="logSavedPath"
	      class="java.lang.String"
	      c:value="${savePath.log.files}"
	/>
	
	
	<!-- repository -->
	<bean id= "MenuMemberDAO"
		  class="home.member.dao.MenuMemberDAOImpl"
		  c:session-ref="sqlSession"
	/>
	<bean id="mybatisMemberDAO"
		  class="home.member.dao.MybatisMemberDAOImpl"
		  c:session-ref="sqlSession"
	/>
	<bean id="searchMybatisMemberDAO"
		  class="home.member.dao.SearchMybatisMemberDAOImpl"
		  c:session-ref="sqlSession"		  		  
	/>
	<bean id="EmployeeDAO"
		  class="home.staff.dao.EmployeeDAOImpl"
		  c:session-ref="sqlSession"
	/>
	<bean id="AnimalCareDAO"
		  class="home.staff.dao.AnimalCareDAOImpl"
		  c:session-ref="sqlSession"
	/>
	<bean id="SearchEmployeeDAO"
		  class="home.staff.dao.SearchEmployeeDAOImpl"
		  c:session-ref="sqlSession"
	/>
	<bean id="noticeDAO"
          class="home.staff.dao.NoticeDAOImpl"
          c:session-ref="sqlSession"
   />
   <bean id="ReceiveDAO"
		 class="home.staff.dao.ReceiveDAOImpl"
   		 c:session-ref="sqlSession"
   />
   <bean id="sharingDAO"
        class="home.member.dao.SharingDAOImpl"
        c:session-ref="sqlSession"
   />


	
	
	
	<!-- service -->
	<bean id="sharingService"
        class="home.member.service.SharingServiceImpl"
        c:sharingDAO-ref="sharingDAO"
    />
	<bean id="ReceiveService"
		  class="home.staff.service.ReceiveServiceImpl"
		  c:receiveDAO-ref="ReceiveDAO"
	/>
	<bean id="noticeService"
          class="home.staff.service.NoticeServiceImpl"
          c:noticeDAO-ref="noticeDAO"
   />

	
	<bean id="searchMemberService"
		  class="home.member.service.SearchMemberServiceImpl"
		  c:searchMemberDAO-ref="searchMybatisMemberDAO"
	/>
	<bean id="memberService"
		  class="home.member.service.MemberServiceImpl"
		  c:searchMemberDAO-ref="searchMybatisMemberDAO"
	/>
	<bean id="AnimalCareService"
		  class="home.staff.service.AnimalCareServiceImpl"
		  c:animalCareDAO-ref="AnimalCareDAO"
	/>
	<bean id="EmployeeService"
		  class="home.staff.service.EmployeeServiceImpl"
		  c:employDAO-ref="EmployeeDAO" 
	/>
	<bean id="MenuMemberService"
		  class="home.member.service.MenuMemberServiceImpl"
		  c:menuMemberDAO-ref="MenuMemberDAO"
	/>
	
	
</beans>
