<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="Activity-Mapper">
	<sql id="select">
		<if test="date-input != null and date-input != ''">
			and where regdate BETWEEN TO_DATE(#{regdateBefore}, 'YYYY-MM-DD') and TO_DATE(#{regdateAfter},'YYYY-MM-DD')
		</if>
	</sql>
	<select id="selectSearchActivityListByAid">
		
	</select>
	<insert id="insertActivity"></insert>
	<update id="updateActivity"></update>
	<delete id="deleteActivity"></delete>
</mapper>









