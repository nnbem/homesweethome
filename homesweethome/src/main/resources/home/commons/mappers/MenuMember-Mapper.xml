<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="MenuMember-Mapper">
    <select id="selectMainMenuMember" resultType="menuMember">
        select *
        from m_menu
        where mCode like '%'||'0000'
        order by mCode asc
    </select>
    <select id="selectSubMenuMember" resultType="menuMember">
        select *
        from m_menu
        where upCode = #{mCode} and isNav=1
    </select>
    <select id="selectMenuMemberByMcode" resultType="menuMember">
        select *
        from m_menu
        where mCode = #{mCode}
    </select>
    <select id="selectMenuMemberByMname" resultType="menuMember">
        select *
        from m_menu
        where mName = #{mName}
    </select>
</mapper>